{
  "name": "se_syntax_highlighter",
  "displayName": "SE Modeling Languages Syntax Highlighter",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
      "id": "csml",
      "aliases": ["CSml", "Concretization and Simbolization Modeling Language"],
      "extensions": [".csml"],
      "configuration": "./language-configuration.json"
      },
      {
      "id": "mcml",
      "aliases": ["MCml", "Merge Conditions Modeling Language"],
      "extensions": [".mcml"],
      "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
      "language": "csml",
      "scopeName": "source.csml",
      "path": "./syntaxes/csml.tmLanguage.json"
      },
      {
      "language": "mcml",
      "scopeName": "source.mcml",
      "path": "./syntaxes/mcml.tmLanguage.json"
      }
    ]
  }
}
