{
  "name": "se-modeling-syntax-highlighter",
  "displayName": "SE Modeling Languages Syntax Highlighter",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.2"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/joaorsmatos02/SE-Modeling-Languages-Syntax-Highlighter"
  },
  "contributes": {
    "languages": [
      {
      "id": "csml",
      "aliases": ["CSml", "Concretization and Simbolization Modeling Language"],
      "extensions": [".csml"],
      "configuration": "./language-configuration.json"
      },
      {
      "id": "mcml",
      "aliases": ["MCml", "Merge Conditions Modeling Language"],
      "extensions": [".mcml"],
      "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
      "language": "csml",
      "scopeName": "source.csml",
      "path": "./syntaxes/csml.tmLanguage.json"
      },
      {
      "language": "mcml",
      "scopeName": "source.mcml",
      "path": "./syntaxes/mcml.tmLanguage.json"
      }
    ]
  },
  "main": "./extension.js",
  "scripts": {
    "lint": "echo No build step"
  }
}
